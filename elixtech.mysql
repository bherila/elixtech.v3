INSERT INTO `n2version` VALUES (2,6,'Start Page',2,32,'2012-10-06 16:50:26',NULL,'2013-03-03 22:12:49','2013-03-03 22:12:50','admin','<?xml version=\"1.0\" encoding=\"utf-16\"?>\r\n<n2 version=\"2.4.0.0\" exportVersion=\"2\" exportDate=\"03/04/2013 06:12:50\">\r\n  <item id=\"0\" name=\"start\" title=\"Start Page\" zoneName=\"\" templateKey=\"\" state=\"32\" created=\"10/06/2012 23:50:26\" updated=\"03/04/2013 06:12:49\" published=\"10/06/2012 23:50:26\" expires=\"03/04/2013 06:12:49\" sortOrder=\"0\" url=\"/?versionIndex=6\" visible=\"True\" savedBy=\"admin\" typeName=\"Dinamico.Models.StartPage, ElixTech.V3\" discriminator=\"StartPage\" versionIndex=\"6\" ancestralTrail=\"/\" alteredPermissions=\"0\" childState=\"0\" versionOf=\"2\">\r\n    <details>\r\n      <detail name=\"Theme\" typeName=\"System.String, mscorlib\" meta=\"\" encoded=\"True\"><![CDATA[Herila]]></detail>\r\n      <detail name=\"Text\" typeName=\"System.String, mscorlib\" meta=\"\" encoded=\"True\"><![CDATA[&lt;h1&gt;Welcome&lt;/h1&gt;\r\n&lt;p&gt;Welcome to my&amp;nbsp;&lt;span style=&quot;text-decoration: line-through;&quot;&gt;blog&lt;/span&gt;&amp;nbsp;home page. I&#39;m currently working on filling it with content, but in the meantime please feel free to browse around :-)&lt;/p&gt;\r\n&lt;h2&gt;Who I am&lt;/h2&gt;\r\n&lt;p&gt;I&#39;m a recent graduate of Brown University who is now a&amp;nbsp;Program Manager&amp;nbsp;at a large software company. I design and build websites and software applications that don&#39;t compete with my employer in my spare time, and also enjoy cooking, skiing, and singing classical choral music as a tenor.&lt;/p&gt;\r\n&lt;h2&gt;Last but not least&lt;/h2&gt;\r\n&lt;p&gt;Thanks for visiting! If you have any questions please feel free to&amp;nbsp;&lt;a href=&quot;http://bherila.net/Contact.aspx&quot;&gt;contact me&lt;/a&gt;&amp;nbsp;- you can use the contact page or, if I&#39;m online, the IM widget floating at the bottom of your browser.&lt;/p&gt;]]></detail>\r\n    